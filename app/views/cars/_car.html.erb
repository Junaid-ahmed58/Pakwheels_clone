<div id="<%= dom_id car %>">
  <% @car.images.each do |image| %>
   <%= image_tag image  %>
  <% end %>

  <p>
    <strong>Vendor:</strong>
    <%= car.vendor %>
  </p>

  <p>
    <strong>Car_Name:</strong>
    <%= car.car_name %>
  </p>

  <p>
    <strong>Model:</strong>
    <%= car.model %>
  </p>

  <p>
    <strong>Engine_Capacity:</strong>
    <%= car.engine_capacity %>
  </p>

  <p>
    <strong>Millage:</strong>
    <%= car.millage %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= car.category %>
  </p>

  <p>
    <strong>Color:</strong>
    <%= car.color %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= car.price %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= car.description %>
  </p>

</div>
